package com.aukey.report.vo;

public class PayDtail {

	// 采购单号
	private String purchase_no;

	// 已付金额
	private Double payment;

	// 支付币别
	private String currency_pay;

	// 汇率
	private Double exchange_rate;

	// 付款人
	private String payer;

	public String getPurchase_no() {
		return purchase_no;
	}

	public void setPurchase_no(String purchase_no) {
		this.purchase_no = purchase_no;
	}

	public Double getPayment() {
		return payment;
	}

	public void setPayment(Double payment) {
		this.payment = payment;
	}

	public String getCurrency_pay() {
		return currency_pay;
	}

	public void setCurrency_pay(String currency_pay) {
		this.currency_pay = currency_pay;
	}

	public Double getExchange_rate() {
		return exchange_rate;
	}

	public void setExchange_rate(Double exchange_rate) {
		this.exchange_rate = exchange_rate;
	}

	public String getPayer() {
		return payer;
	}

	public void setPayer(String payer) {
		this.payer = payer;
	}
}
