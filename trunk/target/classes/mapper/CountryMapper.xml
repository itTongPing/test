<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aukey.report.mapper.CountryMapper">
  <resultMap id="BaseResultMap" type="com.aukey.report.domain.Country">
    <id column="country_id" jdbcType="INTEGER" property="countryId" />
    <result column="country_name" jdbcType="VARCHAR" property="countryName" />
  </resultMap>
  
  <select id="getCountryName" parameterType="map" resultMap="BaseResultMap">
  	select 
  		country_id,
  		country_name
  	from product_ms.country
    where 1=1
  	<if test="countryId != null">
  		and country_id in 
  		<foreach collection="countryId" index="index" item="item"
			open="(" separator="," close=")">
			#{item}
		</foreach>
  	</if>
  </select>
</mapper>